{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "WikiBaseUrl": {
    "en": "https://en.wikipedia.org/wiki/",
    "nl": "https://nl.wikipedia.org/wiki/",
    "es": "https://es.wikipedia.org/wiki/",
    "fr": "https://fr.wikipedia.org/wiki/",
    "de": "https://de.wikipedia.org/wiki/"
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "JWT_Secret": "this_is_my_dummy_secret_development_key",
    "JWT_Issuer": "https://localhost:7072",
    "JWT_Audience": "https://localhost:7072"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "MyNet9Api"
    }
  }
}
